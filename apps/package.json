{
  "name": "apps",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --port 5173",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview --port 5173",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "docs:format": "prettier --write wiki/",
    "docs:dev": "vitepress dev wiki --port 5174",
    "docs:build": "vitepress build wiki",
    "docs:preview": "vitepress preview wiki --port 5174"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^6.0.2",
    "@mdi/js": "^7.4.47",
    "pinia": "^2.2.6",
    "unplugin-vue-markdown": "^0.28.0",
    "vue": "^3.5.13",
    "vue-i18n": "^10.0.5",
    "vue-router": "^4.4.5",
    "vuetify": "^3.7.6"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.9.3",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/eslint-plugin": "1.1.10",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cypress": "^13.16.0",
    "eslint": "^9.14.0",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-vue": "^9.30.0",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^7.0.1",
    "prettier": "^3.3.3",
    "start-server-and-test": "^2.0.8",
    "typescript": "~5.6.3",
    "unplugin-auto-import": "^0.19.0",
    "unplugin-vue-components": "^0.28.0",
    "unplugin-vue-router": "^0.10.9",
    "vite": "^6.0.1",
    "vite-plugin-vue-devtools": "^7.6.5",
    "vite-plugin-vuetify": "^2.0.4",
    "vitepress": "^1.5.0",
    "vitest": "^2.1.5",
    "vue-tsc": "^2.1.10"
  }
}
